   "cell_type": "code",
   "execution_count": 5,
   "metadata": {
    "id": "RRqEwOmHA_4k"
   },
   "outputs": [],
   "source": [
    "filtered_tickers_list = filtered_fin_info[0]\n",
    "filtered_close_price_df = filtered_fin_info[1]\n",
    "filtered_ror_df = filtered_fin_info[2]"
   ]
