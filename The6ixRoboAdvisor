    "def stdev_max(tickers, stdevs):\n",
    "    max_stdev = max(stdevs)\n",
    "    return (tickers[stdevs.index(max_stdev)], max_stdev)\n",
    "max_deviation_stock = stdev_max(filtered_tickers_list, list_of_stdevs)[0]\n",
    "max_deviation_stock"
   ]
