    "def read_in_tickers(filename):\n",
    "    tickers = pd.read_csv(filename, header=None)\n",
    "    tickers.columns = [\"Ticker\"]   #Gives title for tickers column\n",
    "    return tickers.Ticker.tolist()\n",
    "tickers_list = read_in_tickers('Tickers.csv')\n",
    "tickers_list"
   ]
